    {
      "type": "panel",
      "id": "vue3panel",
      "label": "Vue 3 Panel",
      "minimumSize": {
        "width": 200,
        "height": 200
      },
      "maximumSize": {
        "width": 200,
        "height": 400
      },
      "preferredDockedSize": {
        "width": 200,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 200,
        "height": 200
